-- --------------------------------------------------------

--
-- Table structure for table `aae_data_festival`
--

CREATE TABLE IF NOT EXISTS `aae_data_festival` (
  `FID` int(5) NOT NULL AUTO_INCREMENT,
  `name` varchar(64) COLLATE utf8_unicode_ci NOT NULL,
  `alias` varchar(64) COLLATE utf8_unicode_ci NOT NULL,
  `admin` int(5) NOT NULL,
  PRIMARY KEY (`FID`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci AUTO_INCREMENT=5 ;

--
-- Dumping data for table `aae_data_festival`
--

INSERT INTO `aae_data_festival` (`FID`, `name`, `alias`, `admin`) VALUES
(1, 'Freiraum Festival 2016', 'freiraum16', 90),
(2, 'help* Fest 2016', 'helpfest2016', 91),
(3, 'Kunstfest Neustadt 2016', 'kunstfest16', 86),
(4, 'help* Fest 2017', 'helpfest2017', 91);

-- --------------------------------------------------------

--
-- Table structure for table `aae_data_sparte`
--

CREATE TABLE IF NOT EXISTS `aae_data_sparte` (
  `KID` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID für Kategorien',
  `kategorie` varchar(100) NOT NULL COMMENT 'Kategorie für Sparte oder Zielgruppe',
  PRIMARY KEY (`KID`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 COMMENT='Tabelle für Sparten/Zielgruppen' AUTO_INCREMENT=592 ;

